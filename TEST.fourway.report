 ["Data GD",
  '[0-9]+\s+([0-9]+)\s+[0-9]+\s+[0-9]+\s+[0-9]+\s+[0-9a-fA-F]+gcc.debug'],
 ["Data LND",
  '[0-9]+\s+([0-9]+)\s+[0-9]+\s+[0-9]+\s+[0-9]+\s+[0-9a-fA-F]+llvmgcc.nodebug'],
 ["Data LD",
  '[0-9]+\s+([0-9]+)\s+[0-9]+\s+[0-9]+\s+[0-9]+\s+[0-9a-fA-F]+llvmgcc.debug'],
